<% provide(:title, "Contacto") %> 

    
    <div class="contaSty">
        <div class="container">            
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">Cont&aacute;ctenos</h2>
                    <h3 class="section-subheading text-muted">P&oacute;ngase en contacto con el tecn&oacute;logo.</h3>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-6">
                    
                    <div class="form-group">
                       <input type="text" class="form-control" placeholder="Ingrese nombre *" id="name" required="" data-validation-required-message="Por favor ingrese su nombre.">
                        <p class="help-block text-danger"></p>
                    </div>
                    
                    <div class="form-group">
                        <input type="text" class="form-control" placeholder="Ingrese e-mail *" id="email" required="" data-validation-required-message="Por favor ingrese su e-mail.">
                        <p class="help-block text-danger"></p>
                    </div>
                    <div class="form-group">
                        <input type="tel" class="form-control" placeholder="Ingrese asunto *" id="asunto" required="" data-validation-required-message="Por favor ingrese su asunto.">
                        <p class="help-block text-danger"></p>
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="form-group">
                        <textarea class="form-control" placeholder="Ingrese mensaje *" id="message" required="" data-validation-required-message="Por favor ingrese el mensaje." style="resize:none;"></textarea>
                        <p class="help-block text-danger"></p>
                        <div class="col-lg-12 text-center btnmap">
                            <h5 class="section-subheading text-muted">Coordinadora: <font color="Black">Ana Iruleguy</font></h5>
                            <div  style="margin-top: 5px;">
                                <i class="glyphicon glyphicon-envelope">
                                </i> tipay.2009@gmail.com
                            </div>
                            <div  style="margin-top: 5px;">
                                <i class="glyphicon glyphicon-home">
                                    
                                </i> Leandro G&oacute;mez y 33 Orientales S/N.
                            </div>
                        </div>
                    </div>
                </div>
                <div class="clearfix"></div>
                <div class="col-lg-12 text-center">
                    <div id="success"></div>
                    <button type="submit" class="btn btn-default btn-lg">
                        <i class="glyphicon glyphicon-envelope"></i>
                        <span class="network-name">Enviar</span>
                    </button>
                </div>
            </div>
        </div>
        
        
